# Chonost Project - NPM Configuration v2.1.0
# ===================================

# Registry settings
registry=https://registry.npmjs.org/

# Package lock settings
package-lock=true
package-lock-only=false

# Save settings
save=true
save-exact=false
save-prefix=^

# Scripts
ignore-scripts=false
ignore-prepublish=false

# Security
audit=true
audit-level=moderate
fund=false

# Cache settings
cache=~/.npm
cache-min=3600
cache-max=Infinity

# Network settings
fetch-retries=3
fetch-retry-factor=2
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000

# Timeout settings
timeout=300000

# Logging
loglevel=notice
progress=true
silent=false

# Workspace settings (for monorepo)
workspaces=true
workspace-concurrency=4

# Optional dependencies
optional=true

# Production dependencies
production=false
only=null

# Development dependencies
dev=true

# Peer dependencies
legacy-peer-deps=false
strict-peer-dependencies=false

# Package.json validation
json=false

# Git settings
git-checks=true

# Scripts execution
ignore-scripts=false

# Package installation
prefer-offline=false
prefer-online=false

# Package resolution
resolution-mode=highest

# Security settings
audit-level=moderate
fund=false

# Workspace settings
workspaces-update=false

