{
  "name": "@unified/ai-runtime",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "20.17.12",
    "eslint": "9.26.0",
    "@eslint/js": "9.13.0",
    "prettier": "3.3.3",
    "tsx": "4.19.1",
    "typescript": "5.6.3",
    "vitest": "1.6.1"
  },
  "bin": {
    "ai-runtime-complete": "dist/cli.js"
  }
}
